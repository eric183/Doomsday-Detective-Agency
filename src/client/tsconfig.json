{
  "compilerOptions": {
    "jsx": "react",
    "module": "esnext",
    "esModuleInterop": true,
    // "module": "commonjs",
    "declaration": true,
    "moduleResolution": "node",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "../../../dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "composite": true,
    "paths": {
      "~/*":  ["./PaaS/*"],
      "~components/*": ["./PaaS/components/*"],
      "D42_FrontType": ["app.d.ts"],
    },
  }
}
